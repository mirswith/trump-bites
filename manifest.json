{
   "name": "T.Rump Bites",
   "version": "1.0",
   "description": "Making news fun again.",
   "homepage_url": "https://www.buymeacoffee.com/mirswith",
   "icons": {
       "128": "icon128.png"
   },
   "minimum_chrome_version": "37",
   "background": {
     "scripts": ["background.js"]
   },
   "permissions": [
       "storage"
   ],
   "content_scripts": [
       {
           "matches": [
               "http://*/*",
               "https://*/*"
           ],
           "exclude_matches": [
               "https://github.com/*"
           ],
           "js": ["content.js"]
       }
   ],
   "manifest_version": 2
 }
